run = "bash start.sh"
entrypoint = "start.sh"
modules = ["nodejs-22"]

[env]
PATH = "/home/runner/go/bin:${PATH}"
XDG_CONFIG_HOME = "/home/runner/.config"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
